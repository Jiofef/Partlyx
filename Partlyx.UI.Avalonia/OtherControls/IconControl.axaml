<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vmgi="using:Partlyx.ViewModels.GraphicsViewModels.IconViewModels"
             x:Name="Root"
             mc:Ignorable="d" d:DesignWidth="32" d:DesignHeight="32"
             x:Class="Partlyx.UI.Avalonia.IconControl"
             IsVisible="{Binding !#Root.IconContent.IsEmpty}">
        <ContentControl Content="{Binding #Root.IconContent.Content}"/>
</UserControl>
