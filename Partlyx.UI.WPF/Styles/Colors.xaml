<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"                
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Partlyx.UI.WPF">
    
    <!-- For convenience, instead of *light*, *dark* etc. for color shades the number is used. 0 - brightest, 1 darker, 2 even darker etc. -->
    <SolidColorBrush x:Key="PxWhiteBrush" Color="#eef5ff"/>
    <Color x:Key="PxWhiteColor">#eef5ff</Color>

    <SolidColorBrush x:Key="PxBlue0Brush" Color="#b4d4ff"/>
    <Color x:Key="PxBlue0Color">#b4d4ff</Color>

    <SolidColorBrush x:Key="PxBlue1Brush" Color="#86b6f6"/>
    <Color x:Key="PxBlue1Color">#86b6f6</Color>

    <SolidColorBrush x:Key="PxBlue2Brush" Color="#176b87"/>
    <Color x:Key="PxBlue2Color">#176b87</Color>


    <SolidColorBrush x:Key="PxBlackBrush" Color="#37353e"/>
    <Color x:Key="PxBlackColor">#37353e</Color>

    <SolidColorBrush x:Key="PxGrayBrush" Color="#777b80"/>
    <Color x:Key="PxGrayColor">#777b80</Color>

</ResourceDictionary>